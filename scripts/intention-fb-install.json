{
  "event": {
    "provider": "fluentbit-jenkins-job",
    "reason": "Job triggered",
    "url": "JOB_URL"
  },
  "actions": [
    {
      "action": "server-access",
      "id": "login",
      "provision": ["token/self"],
      "service": {
        "name": "fluent-bit",
        "project": "fluent",
        "environment": "production",
        "target": {
          "name": "jenkins-polaris",
          "project": "jenkins",
          "environment": "production"
        }
      }
    },
    {
      "action": "package-installation",
      "id": "install",
      "provision": [],
      "cloud": {
        "target": {
          "provider": "dxcas",
          "instance": {
            "name": ""
          }
        }
      },
      "service": {
        "name": "fluent-bit",
        "project": "fluent",
        "environment": "production"
      },
      "package": {
        "version": "2.73.2"
      }
    },
    {
      "action": "package-provision",
      "id": "provision",
      "provision": ["approle/secret-id"],
      "service": {
        "name": "fluent-bit",
        "project": "fluent",
        "environment": "production"
      }
    }
  ],
  "user": {
    "name": "USER_ID"
  }
}